#include <iostream>
using namespace std;
void Sovershenoe(int a , int b)
{
    int sum = 0;
    for (int i = a; i < b+1; i++)
    {
        for (int j = 1; j < i; j++)
        {
            
            if (i % j == 0 )
            {
                sum += j;
            }
            if (sum == i)
            {
                cout << "Число совершеное" << endl;
                cout << sum << endl;
                
            }
            
        }
        sum = 0;
        
    }
    
}


int main()
{
    setlocale(LC_ALL, "rus");
    int a = 1;
    int b = 6;
    Sovershenoe(a,b);

    
}
