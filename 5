#include <iostream>
#include <cstdlib> 
using namespace std;

void Lucky(string a)
{
    int b = 0;
    char c;
    int b2 = 0;
    for (int i = 0; i < 3; i++)
    {
        c = a[i];

        b += atoi(&c);
    }
    for (int i = 3; i < 6; i++)
    {
        c = a[i];
        b2 += atoi(&c);
    }
    if (b == b2)
    {
        cout << "Число счастливое" << endl;
        
    }
    else
    {
        cout << "Число не счастливое" << endl;
    }

}

int main()
{
    setlocale(LC_ALL, "rus");
    string b = "343344";
    Lucky(b);


    
}
